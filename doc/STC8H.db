[occupier]
std{
    uart1 alias stc8h.uart1;

}

stc8h{
    uart1{
        switch $IF_USE mutex{
            option $USE;
            option $NOT_USE;
        }
        switch $USE_PIN mutex rely not $NOT_USE{
            option $PIN4_PIN6 occupy std.pin4,std.pin6;
            option $PIN8_PIN10 occupy std.pin8,std.pin10 rely $;
        }
        input $LABEL int(0,65535) occupy checker;
        input $LABEL1 ascii occupy checker;
        input $LABEL2 utf8 occupy checker;
    }
}

[resource]
std{
    pin0 nonshare;
    clock
}

[package]

[label]
USE = {
    smp : 启用
    trd : 啟用
    jap : 使用
    eng : 
}

关键字：alias switch option mutex rely and not or input nonshare occupy